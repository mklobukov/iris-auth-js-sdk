{
  "name": "iris-auth-js-sdk",
  "version": "1.0.0",
  "description": "JS SDK for Iris Auth Manager",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist src",
    "build:umd": "webpack --output-filename index.auth.js",
    "build:umd.min": "webpack --output-filename index.auth.min.js -p"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:robjsliwa/iris-auth-js-sdk.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "author": "Rob Sliwa <robjsliwa@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/robjsliwa/iris-auth-js-sdk",
  "dependencies": {
  
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-2": "6.11.0",
    "json-loader": "0.5.4",
    "npm-run-all": "2.3.0",
    "rimraf": "2.5.3",
    "webpack": "1.13.1"
  }
}
